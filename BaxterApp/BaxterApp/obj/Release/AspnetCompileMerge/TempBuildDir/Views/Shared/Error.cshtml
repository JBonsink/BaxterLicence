@model Baxter.Models.Baxter.ErrorModel

@{
    ViewBag.Title = "Error";    
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<p style="margin:15px;font-size:large">
    @Html.DisplayFor(model => model.Message);
    @if(!string.IsNullOrEmpty(Model.StackTrace))
    {
        <hr style="margin:15px;"/>
        <p style="margin:15px;">@Model.StackTrace</p>
    }
</p>